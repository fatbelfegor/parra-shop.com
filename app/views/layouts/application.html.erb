	    </div>
	    <%= form_for :order, {url: orders_path, html: {onsubmit: 'return orderValidate(this)'}} do |f| %>
	    <div class="row">
	    	<span class="col-md-5 text-right"><%= f.label :first_name, "Ваше имя:", {class: 'control-label wrath-content-box', style: 'padding:0 15px'} %></span>
	    	<div class="col-md-5">
		      <div class="wrath-content-box" style="padding:0 15px">
				<%= f.text_field :first_name, class: "form-control" %>
		      </div>
		    </div>
		</div>
		<div class="row">
		    <span class="col-md-5 text-right"><nobr><%= f.label :phone, "Контактный телефон:", {class: 'control-label wrath-content-box', style: 'padding:0 15px'} %></nobr></span>
	    	<div class="col-md-5">
		      <div class="wrath-content-box" style="padding:0 15px">
				<%= f.text_field :phone, class: "form-control" %>
		      </div>
		    </div>
	    </div>
		<div class="row">
			<span class="col-md-5 text-right"><nobr><%= f.label :email, "Ваш e-mail:", {class: 'control-label wrath-content-box', style: 'padding:0 15px'} %></nobr></span>
			<div class="col-md-5">
		      <div class="wrath-content-box" style="padding:0 15px">
				<%= f.text_field :email, class: "form-control" %>
		      </div>
		    </div>
	    </div>
	    <div class="checkbox">
		  <label>
		    <input type="checkbox" onclick="orderShowAll()">
		    <span class="text-left">Нужна доставка</span>
		  </label>
		</div>
	    <div id="otherInputs">
		    <div class="row">
		    <span class="col-md-5 text-right"><nobr><%= f.label :addr_street, "Улица:", {class: 'control-label wrath-content-box', style: 'padding:0 15px'} %></nobr></span>
	    	<div class="col-md-5">
		      <div class="wrath-content-box" style="padding:0 15px">
		        <%= f.text_field :addr_street, class: "form-control" %>
		      </div>
		    </div>
		    </div>
		    <div class="row">
		    <span class="col-md-5 text-right"><nobr><%= f.label :addr_home, "Дом:", {class: 'control-label wrath-content-box', style: 'padding:0 15px'} %></nobr></span>
	    	<div class="col-md-5">
		      <div class="wrath-content-box" style="padding:0 15px">
		        <%= f.text_field :addr_home, class: "form-control" %>
		      </div>
		    </div>
		    </div>
		    <div class="row">
